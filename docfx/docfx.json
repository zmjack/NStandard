{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "CNAME",
          "logo.svg",
          "home.svg"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "NStandard",
      "_appLogoPath": "logo.svg",
      "_appLogoUrl": "/",
      "_enableSearch": true,
      "pdf": false
    }
  }
}